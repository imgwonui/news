{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/cron",
      "dest": "api/cron.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    },
    "api/cron.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/cron",
      "schedule": "0 21 * * *"
    }
  ]
}
